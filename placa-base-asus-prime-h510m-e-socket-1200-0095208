<script>  
            window.onload = function() {
              var addStylesNode = document.getElementById("onload-styles");
              var replacement = document.createElement("div");
              replacement.innerHTML = addStylesNode.textContent;
              document.body.appendChild(replacement);
              addStylesNode.parentElement.removeChild(addStylesNode);
            };
            
          </script>

<script src="https://cdn2.depau.es/assets/js/jQuery-3.3.1/jquery-3.3.1.min.js"></script>
<script src="https://cdn2.depau.es/assets/js/jQuery-validate/jquery.validate.min.js"></script>
<script src="https://cdn2.depau.es/assets/js/jQuery-numeric/jquery.numeric.min.js" async></script>
<script src="https://cdn2.depau.es/assets/js/jQuery-placeholder/jquery.placeholder.min.js"></script>
<script src="https://cdn2.depau.es/assets/js/jQuery-ui/1.12.1/jquery-ui.min.js"></script>

<script src="https://cdn2.depau.es/assets/js/bootstrap/4.1.1/js/bootstrap.min.js"></script>
<script src="https://cdn2.depau.es/assets/js/owlCarousel/owl.carousel.js" async></script>
<script type="text/javascript">
        $(window).on('load', function(){
                
        //Inicializamos componentes/        
        init_core();
        ini_validate();
        ini_modal();
        
        //Deferload. Si está definido jPages se Ejecuta automaticamente en su callback(), ini(), etc...
        if(!$.fn.jPages){ 
            deferload(); 
        }

        function showModal(){
            $('#fade, light').show();
            $('#fade').addClass('visible');
        }
        function removeModal(){
            $('#fade, #light').remove();
        }        
        function closeModal(){
            $('#fade, #light-close, .close-modal').click(function(){
                removeModal();
            });
        }
        
        //Funciones genericas
        function init_core(){
            //Tooltip.
            $('.d-tooltip span.tip').each(function(){
                if($(this).width() < 90){
                    $(this).css('margin-left','-45px');
                }
            });
            
            //Bs-Tooltip (Simulado)
            $('.bs-tooltip').each(function(i, e){
                $(this).css('position', 'relative').css('display', 'block');
                var $span = $('<span data-tooltip-id='+i+'>').appendTo($(this)).text($(this).data('title'));
                $span
                    .addClass('new-tip')
                    .css('position', 'absolute')
                    .css('z-index', '10')
                    .css('display', 'none')
                    .css('top', '-30px')
                    .css('white-space', 'nowrap');
                
                //Left lo sacamos aqui porque necesitamos que ya haya ocupado espacio
                var left = ($span.width()/2) - ($(this).width()/2) +5;
                $span.css('left', -left +'px');
                $(this).hover( //Simulamos Toggle
                    function(){ $('span[data-tooltip-id="'+i+'"]').show(); },
                    function(){ $('span[data-tooltip-id="'+i+'"]').hide();}
                );
            });

            //Blink
            $('.blink').each(function() {
                var blink_element = $(this);
                setInterval(function() {
                    blink_element.toggleClass('blink');
                }, 500);
            });
            
            $('.blink_white').each(function() {
                var blink_element = $(this);
                setInterval(function() {
                    blink_element.toggleClass('blink_white');
                }, 500);
            });
            
            //ColorBlink
            $('#tabs span[data-active],a[data-active]').each(function() {
                var blink_element = $(this);
                setInterval(function() {
                    if ($(blink_element).attr('data-active') == 'primary') {
                        $(blink_element).attr('data-active','secondary');
                        $(blink_element).removeClass($(blink_element).attr('data-primary'));
                        $(blink_element).addClass($(blink_element).attr('data-secondary'));
                    } else {
                        $(blink_element).attr('data-active','primary');
                        $(blink_element).removeClass($(blink_element).attr('data-secondary'));
                        $(blink_element).addClass($(blink_element).attr('data-primary'));
                    }
                }, 500);
            });
             
            //Mostrar/Ocultar claves
            //  Necesitamos un checkbox(mostrar claves) con el atributo data-password
            //  Y en cada input text/password que tengamos contraseñas el atributo data-password
            $("input[type='checkbox'][data-password]").change(function(){
                var valor = $(this).prop('checked');
                if(valor == true){
                    $("input[type='password'][data-password]").attr('type', 'text');
                }else if(valor == false){
                    $("input[type='text'][data-password]").attr('type', 'password');
                }
            });
            
            
            //DATEPICKER
            if($.datepicker){
                
                jQuery(function($){
                    $.datepicker.regional['es'] = {
                        closeText: 'Cerrar',
                        prevText: '&#x3c;Ant',
                        nextText: 'Sig&#x3e;',
                        currentText: 'Hoy',
                        monthNames: ['Enero','Febrero','Marzo','Abril','Mayo','Junio',
                        'Julio','Agosto','Septiembre','Octubre','Noviembre','Diciembre'],
                        monthNamesShort: ['Ene','Feb','Mar','Abr','May','Jun',
                        'Jul','Ago','Sep','Oct','Nov','Dic'],
                        dayNames: ['Domingo','Lunes','Martes','Mi&eacute;rcoles','Jueves','Viernes','S&aacute;bado'],
                        dayNamesShort: ['Dom','Lun','Mar','Mi&eacute;','Juv','Vie','S&aacute;b'],
                        dayNamesMin: ['Do','Lu','Ma','Mi','Ju','Vi','S&aacute;'],
                        weekHeader: 'Sm',
                        dateFormat: 'dd/mm/yy',
                        firstDay: 1,
                        isRTL: false,
                        showMonthAfterYear: false,
                        yearSuffix: ''
                    };
                    $.datepicker.setDefaults($.datepicker.regional['es']);
                });
                
                //HOOK DatePicker
                $('.datepicker').datepicker();
                
            }
            
           
            
        }
        
        
        (function() {
            $('.vista-favoritos + #listado-articulos .caja-fav[data-modal]').removeAttr('data-modal');
            var siteUrl = 'https://www.depau.es/';
            var strLayout = "<div id=\"fade\" class=\"overlay\"><\/div>\r\n\r\n<div id=\"light\">\r\n        <span id=\"light-close\"><i class=\"fal fa-times\"><\/i><\/span>\r\n        <div id=\"modal-light-container\" class=\"border-radius-all\">\r\n\r\n        <div id=\"light-wrap\" class=\"light-wrap\">\r\n\r\n            <div class=\"light-header border-radius-top\">\r\n            <\/div>\r\n\r\n            <div id=\"contenido_modal\">\r\n                            <\/div>\r\n        <\/div>\r\n\r\n\r\n    <\/div>\r\n<\/div>";
            var strConfirm = "<div class='margin-top-xl text-center'><div id='data-modal-confirm' class='btn tercio btn-d-primary'>Si</div><div class='btn tercio btn-d-primary margin-left-xs close-modal'>No</div></div>";
            var settings = {confirm: false, lock:false, title:'', dataType: 'ajax', strData: false, strLocation: false };
            
            appModal = {

                options: {},
                
                init: function(options){
                    this.handleModal();
                },
                //Funciones Básicas
                setOptions: function(options){
                    this.options = {};
                    $.extend(this.options, settings, options);
                },
                showModal: function (){
                    $('#fade, #light').show();
                    $('#fade').addClass('visible');
                },
                removeModal: function(){
                    $('#fade, #light').remove();
                },       
                //$.extend(appModal, { modalCallback: function(){} });
                modalCallback: function(){                
                },
                loadModal: function(options){
                    
                    this.setOptions(options);
                    
                    if(this.options.dataType==='ajax'){
                        var self = this;
                        if(this.options.dataMethod==='post'){
                            $.ajax({
                                url: siteUrl+self.options.dataUrl,
                                type: 'POST',
                                data: this.options.postValues,
                                success: function(data) {
                                    self.render(data);
                                }
                            });
                        }else{       
                            $.ajax({
                                url: siteUrl+self.options.strLocation.substring(1),
                                success: function(data) {
                                    self.render(data);
                                }
                            });
                        }
                    }else{
                        this.render(this.options.strData);
                    }
                },
                render: function(strData){
                    this.removeModal();
                    if(this.options.dataType==='ajax'){
                        $('body').prepend(strData);
                    }else{
                        $('body').prepend(strLayout);
                        $('#contenido_modal').prepend(strData);
                    }
                    this.options.title && $('#light-wrap h1').text(this.options.title);
                    this.showModal();
                    //Callbacks
                    this.handleClose();
                    ini_validate();
                    this.modalCallback();
                },

                //Manejo Básico
                handleModal: function(){
                    var self=this;
                    //Modales
                     $('span[data-modal-loading]').click(function(e){
                        e.preventDefault();
                        var options = self.parseTriggerAttr($(this));
                        self.loadModal(options);
                        document.location = $(this).attr('href');
                        setTimeout(function() {
                            self.removeModal();
                        }, 30000);
                    });
                    $('body').on('click', '[data-modal]', function(e){
                        e.preventDefault();
                        var options = self.parseTriggerAttr($(this));
                        self.loadModal(options);
                    });
                    $('body').on('click', '[data-modal-post]', function(e){
                        e.preventDefault();
                        var options = self.parseTriggerAttr($(this));
                        options.dataType = 'ajax';
                        options.dataMethod = 'post';
                        options.dataUrl = $(this).data('modal-post');
                        options.postValues = {data: $(this).data('post')};
                        self.loadModal(options);
                    });
                    //Modal Confirm
                    $('a[data-modal-confirm]').click(function(e){
                        e.preventDefault();
                        var options = self.parseTriggerAttr($(this));
                        self.loadModal(options);
                        self.handleConfirm();
                    });
                    this.handleClose();
                },
                handleClose: function(){
                    var self=this;
                    $('#fade').click(function(){
                        if(self.options.lock!==true
                                && self.options.hardlock!==true
                                ){
                            self.removeModal();
                        }
                    });
                    $('#light-close').click(function(){
                        if(self.options.hardlock!==true){
                            self.removeModal();
                        }
                    });
                    $('.close-modal').click(function(){ 
                        self.removeModal();
                    });                
                },
                handleConfirm: function(){
                    var self=this;
                    $("#data-modal-confirm").click(function(){
                        location.href = self.options.strLocation;
                    });             
                },
                parseTriggerAttr: function($trigger){

                    var options = {};
                    options.lock     = ($trigger.data('modal-lock')===true);
                    options.hardlock = ($trigger.data('modal-hard-lock')===true);
                    options.title = $trigger.data('modal-title');
                    
                    //Modal
                    if($trigger.data('modal') && $trigger.attr('data-modal').substr(0,1)==='/'){
                        options.dataType = 'ajax';
                        options.strLocation = $trigger.attr('data-modal');
                    }else{
                        options.dataType = 'str';
                        options.strData = $trigger.attr('data-modal');
                    }
                    
                    // Modal Loading
                    if($trigger.data('modal-loading')){
                        options.dataType = 'ajax';
                        options.strLocation = $trigger.data('modal-loading');
                        options.strData = $trigger.data('modal-loading');
                    }
                    
                    //Modal Confirm
                    if($trigger.data('modal-confirm')){
                        options.confirm = true;
                        options.dataType = 'str';
                        options.strLocation = $trigger.attr('href');
                        options.strData = '<h2 class="margin-top-xl text-center">' + $trigger.data('modal-confirm') + '</h2>' + strConfirm;
                    }
                    
                    return options;
                }
            };
        })();
        
        appModal.init();

        //TODO: Refactorizar y eliminar
        //Manejo de funciones y disparadores Modales
        function ini_modal(){

            closeModal();    
            
            // Modal Carrito
            $('body').on('click', '[data-modal-cart]', function(e){
                e.preventDefault();
                
                var codigo = $(this).data('codigo');
                var cantidad = parseInt($("input[name='cantidad'][data-codigo='" + codigo + "']").val());
                if (cantidad < 0){
                    cantidad= -cantidad;
                }
                var deposito = $(this).data('deposito');

                $.post('/presupuestos/inserta_presupuesto', { articulo: codigo, cantidad: cantidad, deposito:deposito }, 
                    function(data){                        
                        var strModal = data;
                        removeModal();
                        $('body').prepend(strModal);
                        showModal();
                        closeModal();
                        //Actualizamos Lengueta Carrito (Esté visible o no)
                        var total_items = parseInt($("#carrito_drop .lenguetas-number").html());
                        $("#carrito_drop .lenguetas-number").html(total_items+cantidad);
                        $("#carrito_drop_2 .lenguetas-number").html(total_items+cantidad);
                        $(".header .icon-carrito span").html(total_items+cantidad);
                        
                    });
            });            
            
            // Modal Avisame
            $('body').on('click', '[data-modal-avisame]', function(e){
                e.preventDefault();                
                var codigo = $(this).attr('data-codigo');
                $.post('/avisame', { articulo: codigo },
                    function(data){
                        var strModal = "\
                            <div id='fade' class='overlay'></div>\n\
                            <div id='light'>\n\
                                <span id='light-close'><i class='fal fa-times'></i></span>\n\
                                <div id='modal-light-container' class='border-radius-all'>\n\
                                    <div id='light-wrap' class='light-wrap'>\n\
                                        <div class='light-header border-radius-top'>\n\
                                        </div>\n\
                                        <div id='contenido_modal'>\n\
                                            <h2 class='cabecera-modal-nueva'>Depau Sistemas S.L.</h2>\n\
                                            "+data+"\n\
                                        </div>\n\
                                    </div>\n\
                                </div>\n\
                            </div>";
                        removeModal();
                        $('body').prepend(strModal);
                        showModal();
                        closeModal();
                    });
            });

            // Modal '+ info'
            $('body').on('click', '[data-modal-mas-info]', function(e) {

                e.preventDefault();
                var codigo = $(this).attr('data-codigo');
                $.post(
                    '/ajaxrequest/ajax_mas_info',
                    { articulo: codigo },
                    function(data) {
                        var strModal = "\
                            <div id='fade' class='overlay'></div>\n\
                            <div id='light'>\n\
                                <span id='light-close'><i class='fal fa-times'></i></span>\n\
                                <div id='modal-light-container' class='border-radius-all'>\n\
                                    <div id='light-wrap' class='light-wrap'>\n\
                                        <div class='light-header border-radius-top'>\n\
                                        </div>\n\
                                        <div id='contenido_modal'>\n\
                                            <h2 class='cabecera-modal-nueva'>Depau Sistemas S.L.</h2>\n\
                                            <div class='col-xs-12 text-center'>\n\
                                                <div class='form-group'>\n\
                                                    <p>Lo sentimos, pero no sabemos si este artículo volverá a estar disponible, ni cuándo.</p>\n\
                                                    <p class='avisame text-center'>Si quieres, tu Comercial puede avisarte si vuelve a estar disponible.</p>\n\
                                                    <a href='#' class='link-avisame' data-modal-avisame='true' data-codigo='" + codigo + "'>\n\
                                                        <span class='stock-almacen btn btn-info' style='margin-top:15px;'>Quiero que me avise</span>\n\
                                                    </a>\n\
                                                </div>\n\
                                            </div>\n\
                                            <p class='botones-modal'>\n\
                                                <a class='btn cuarto btn-d-primary close-modal'>Cerrar</a>\n\
                                            </p>\n\
                                        </div>\n\
                                    </div>\n\
                                </div>\n\
                            </div>";
                        removeModal();
                        $('body').prepend(strModal);
                        showModal();
                        closeModal();
                    });
            });

            $('#finalizar_pedido').click(function(){
                $('#formDireccionEnvio').submit();
            });
            
            // Formulario de acceso a máscara
            $('[data-login]').click(function(e){
                e.preventDefault();
                var action = $(this).attr('data-login');
                var strModal = "\
                    <div id='fade' class='overlay'></div>\n\
                    <div id='light'>\n\
                        <span id='light-close'><i class='fal fa-times'></i></span>\n\
                        <div id='modal-light-container' class='border-radius-all'>\n\
                            <div id='light-wrap' class='light-wrap'>\n\
                                <div class='light-header border-radius-top'>\n\
                                </div>\n\
                                <div id='contenido_modal'>\n\
                                    <h2 class='cabecera-modal-nueva'>Depau Sistemas S.L.</h2>\n\
                                    <form id='formAccesoMascara' method='post' action='"+action+"' autocomplete='off'>\n\
                                        <div class='margin-top-xl'>\n\
                                            <p class='margin-top-x1 text-center'>Por favor, introduce tu contraseña para comprobar tu identidad:</p>\n\
                                            <p class='margin-top-x1 text-center'>\n\
                                                    <label class='cuarto'><em>*</em> Contraseña</label>\n\
                                                    <input type='password' class='cuarto' value='' id='password' name='password' placeholder='Contraseña'>\n\
                                            </p>\n\
                                            <p class='rojo text-center'><em>*</em> Campos requeridos</p>\n\
                                        </div>\n\
                                        <p class='botones-modal'>\n\
                                            <button class='btn next cuarto btn-d-primary'>Enviar</button>\n\
                                        </p>\n\
                                </form>\n\
                                </div>\n\
                            </div>\n\
                        </div>\n\
                    </div>";
                removeModal();
                $('body').prepend(strModal);
                showModal();
                closeModal();
                // Establecemos validadores.
                // TODO: Añadir en el mismo formulario.
                $('#formAccesoMascara').validate({
                    errorElement: '',
                    highlight: function(element){
                        $(element).addClass('error_bg');
                    },
                    unhighlight: function(element){
                        $(element).removeClass('error_bg');
                    },
                    rules: {
                        password: { required: true }
                    }
                });
                $('#password').focus();
            });
          

            // Formulario de envío de email máscara
            $('[data-email-mascara]').click(function(e){
                e.preventDefault();
                var action = $(this).attr('data-email-mascara');        
                var strModal = "\
                    <div id='fade' class='overlay'></div>\n\
                    <div id='light'>\n\
                        <span id='light-close'><i class='fal fa-times'></i></span>\n\
                        <div id='modal-light-container' class='border-radius-all'>\n\
                            <div id='light-wrap' class='light-wrap'>\n\
                                <div class='light-header border-radius-top'>\n\
                                </div>\n\
                                <div id='contenido_modal'>\n\
                                    <h2 class='cabecera-modal-nueva'>Depau Sistemas S.L.</h2>\n\
                                    <form id='formEmailMascara' method='post' action='"+action+"' autocomplete='off'>\n\
                                    <div>\n\
                                        <p class='margin-top-x1 text-center'>\n\
                                            <label class='cuarto'><em>*</em> Email de envío</label>\n\
                                            <input type='text' class='medio' value='' id='email' name='email' placeholder='email'>\n\
                                        </p>\n\
                                        <p class='margin-top-x1 text-center'>\n\
                                            <label class='cuarto' style='vertical-align:top'>&nbsp;&nbsp;Observaciones</label>\n\
                                            <textarea class='medio' id='observaciones' name='observaciones'></textarea>\n\
                                        </p>\n\
                                        <p class='rojo text-center'><em>*</em> Campos requeridos</p>\n\
                                    </div>\n\
                                    <p class='botones-modal'>\n\
                                        <button class='btn next cuarto btn-d-primary'>Enviar</button>\n\
                                    </p>\n\
                                </form>\n\
                                </div>\n\
                            </div>\n\
                        </div>\n\
                    </div>";                        
                removeModal();
                $('body').prepend(strModal);
                showModal();
                closeModal();
                // Establecemos validadores.
                // TODO: Añadir en el mismo formulario.
                $('#formEmailMascara').validate({
                    errorElement: '',
                    highlight: function(element){
                        $(element).addClass('error_bg');
                    },
                    unhighlight: function(element){
                        $(element).removeClass('error_bg');
                    },
                    rules: {
                        email: { required: true, email: true }
                    }
                });
                $('#email').focus();
            });
            
            // Formulario de acceso a máscara
            $('[data-condiciones-1330]').click(function(e){
                e.preventDefault();
                var action = $(this).attr('data-login');         
                var strModal = "\
