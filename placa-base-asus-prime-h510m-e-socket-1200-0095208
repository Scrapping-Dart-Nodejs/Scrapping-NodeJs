            // Modal añadir al presupuesto
            $("body").on("click", '[data-inserta-presupuesto-mascara]', function(e){
                e.preventDefault();                
                var codigo = $(this).attr('data-codigo');
                var cantidad = Math.abs(parseInt($("input[name='cantidad'][data-codigo='" + codigo + "']").val()));
                cantidad = ($.isNumeric(cantidad)) ? cantidad : 1;
                $.post('/usuarios/inserta_presupuesto', { articulo: codigo, cantidad: cantidad }, 
                    function(data){
                        var strModal = "\
                    <div id='fade' class='overlay'></div>\n\
                    <div id='light'>\n\
                        <span id='light-close'><i class='fal fa-times'></i></span>\n\
                        <div id='modal-light-container' class='border-radius-all'>\n\
                            <div id='light-wrap' class='light-wrap'>\n\
                                <div class='light-header border-radius-top'>\n\
                                </div>\n\
                                <div id='contenido_modal'>\n\
                                    <h2 class='cabecera-modal-nueva'>Depau Sistemas S.L.</h2>\n\
                                    "+data+"\n\
                                </div>\n\
                            </div>\n\
                        </div>\n\
                    </div>";
                        removeModal();
                        $('body').prepend(strModal);
                        showModal();
                        closeModal();
                    });
            });
            
        }
        
        
        
        
        //Extendemos Validate
        function ini_validate(){

            if ($.validator) {
                
                //DEFAULT OPTIONS
                $.extend( $.validator.defaults, {
                    //errorClass: "error_bg",
                    //validClass: "success_bg",
                    errorPlacement: function (error, element) { 
                        //element.type //element.name
                        if( $(element).attr('data-label') === 'false' ){
                            error.remove();
                        }else if( $(element).attr('data-label') === 'garantia' ){
                            var element_insertar = $(element).next('label');
                            $(error).css('margin-left', $(element).prev('label').width()).css('width', '100%').css('font-weight', 'normal');
                            error.insertAfter(element_insertar);
                            
                        }else if( $(element).attr('data-label') === 'garantia-recogida' ){
                            var element_insertar = $(element).next('label');
                            $(error).css('margin-left', $(element).prev('label').width()).css('font-weight', 'normal');
                            error.insertAfter(element_insertar);
                            
                        }else{
                            $(error).css('margin-left', $(element).prev('label').width()).css('width', '100%').css('font-weight', 'normal');
                            error.insertAfter(element);
                        }
                    },
                    highlight: function(element, errorClass, validClass) {
                        
                        if(element.type === 'checkbox'){
                            $(element).parent().addClass('error_bg_wrapp');
                        }else if (element.type === 'radio') {
                            this.findByName(element.name).addClass('error_bg');
                        } 
                        else if ($(element).attr('data-validate') === 'recaptcha'){
                            $(element).parent().addClass('error_bg');
                        } 
                        else {
                            $(element).addClass('error_bg');
                        }
                        
                    },
                    unhighlight: function(element, errorClass, validClass) {
                        
                        if(element.type==='checkbox'){
                            $(element).parent().removeClass('error_bg_wrapp');
                        }
        
                        if($(element).attr('onevalueminimun')){
                            $('input[onevalueminimun]').removeClass('error_bg');
                        }
                        
                        if ($(element).attr('data-validate') === 'recaptcha'){
                            $(element).parent().removeClass('error_bg');
                        } 
                        
                        if ($(element).attr('data-label') === "false") {

                            $(element).removeClass('error_bg');
                           
                        } else {
                            if (element.type === 'radio') {
                                this.findByName(element.name).removeClass('error_bg');
                            } else {
                                $(element).removeClass('error_bg');
                            }
                            
                        }
                    }
                });
                
                //Sobreescribimos mensajes
                $.extend($.validator.messages, {
                    required: 'Campo Obligatorio.',
                    remote: 'Por favor compruebe este campo.',
                    email: 'Introduzca un email válido.',
                    url: 'Introduzca una URL válida.',
                    date: 'Introduzca una fecha válida.',
                    dateISO: 'Introduzca una fecha válida (ISO).',
                    number: 'Introduzca un número válido.',
                    digits: 'Introduzca sólo dígitos.',
                    creditcard: 'Por favor introduzca un número de tarjeta válido.',
                    equalTo: 'Los campos no coinciden.',
                    accept: 'Introduzca un archivo con extensión válida.',
                    maxlength: jQuery.validator.format('Máximo {0} caracteres.'),
                    minlength: jQuery.validator.format('Mínimo {0} caracteres.'),
                    rangelength: jQuery.validator.format('Introduzca un valor entre {0} y {1} caracteres.'),
                    range: jQuery.validator.format('Introduzca un valor entre {0} y {1}.'),
                    max: jQuery.validator.format('Introduzca un valor igual o menor a {0}.'),
                    min: jQuery.validator.format('Introduzca un valor igual o mayor a {0}.'),
                    extension: jQuery.validator.format('Solo admitidos archivos con extensión {0}')
                });
                
                //ADD METHODS
                
                //Validador campo existente
                jQuery.validator.addMethod('exists', function(value, element, params){ 
                        var result = true;    
                        var model = $(element).attr('data-exists');
                        var field = $(element).attr('name');

                        $.ajax({
                            url: '/ajaxrequest/check_exists',
                            type: 'POST',
                            async: false,
                            dataType: 'json',
                            data: {value: value, model: model, field:field },
                            success: function(data, textStatus, jqXHR) {
                                result = data;
                            }
                        });
                        return result;
                    },
                    jQuery.validator.format('{0} no válido.')
                );

                //Validador campo único
                jQuery.validator.addMethod('unique', function(value, element, params){ 
                        if(value==$(element).attr('data-current')){
                            return true;
                        }
                        var result = true;    
                        var model = $(element).attr('data-unique');
                        var field = $(element).attr('name');

                        $.ajax({
                            url: '/ajaxrequest/check_unique',
                            type: 'POST',
                            async: false,
                            dataType: 'json',
                            data: {value: value, model: model, field:field },
                            success: function(data, textStatus, jqXHR) {
                                result = data;
                            }
                        });
                        return result;
                    },
                    jQuery.validator.format('El {0} ya existe.')
                );

                //Validador un campo como mínimo
                jQuery.validator.addMethod('onevalueminimun', function(value, element, params){
                        var result = false;
                        
                        $('input[onevalueminimun]').each(function(){
                            var type = $(this).attr('onevalueminimun');
                            
                            switch(type){
                                case 'checkbox':
                                    result = result || $(this).prop('checked');
                                    break;
                                case 'numeric':
                                    result = result || ($(this).val() > 0);
                                    break;
                                default:
                                    result = result || $(this).val() !== '';
                                    break;
                            }
        
                        });
                        /**
                        if (result) {
                            $('input[onevalueminimun]').each(function(){
                                var name = $(this).attr('name');
                                $('label.[for=\"'+name+'\"]').hide();
                                $(this).removeClass('error_bg');
                            });
                        }
                        */
                        return result;
                    },'Al menos uno de los campos debe contener un valor.'
                );

                //Validador de enteros
                jQuery.validator.addMethod('integer', function(value){                    
                       var patron = /^\d*$/; 
                       return patron.test(value);
                       //return value.match(patron);                   
                    }, 'Sólo números enteros.'
                );

                //Validador de decimales (positivos)    
                jQuery.validator.addMethod('positiveNumber', function (value){
                        return Number(value) >= 0;
                    },'Introduzca un número válido.'
                );

                //Validador de fecha
                jQuery.validator.addMethod('fecha', function(value) {
                        return value.match(/^\d\d?\/\d\d?\/\d\d\d\d$/);
                    },'El formato debe ser dd/mm/yyyy'
                );

                // Dimensiones de imagen.
                $('input[type="file"][imgsize="true"]').each(function(){
                    $(this).on('change',function(){
                        input = this;
                        $(input).removeAttr('data-currentwidth');
                        $(input).removeAttr('data-currentheight');
                        fr = new FileReader();
                        $(fr).load(function(){
                            var img = document.createElement('img');
                            $(img).css('display','none');
                            $(img).attr('src',fr.result);
                            $(img).load(function() {
                                $(input).attr('data-currentwidth',this.width);
                                $(input).attr('data-currentheight',this.height);
                                $(this).parent().remove(img);
                            });
                            $(this).parent().append(img);
                        });
                        fr.readAsDataURL(input.files[0]);
                    });
                });

                jQuery.validator.addMethod('imgsize', function(value, element, params){
                    jQuery.validator.messages.imgsize = "Tamaño: "+$(element).attr('data-width')+"x"+$(element).attr('data-height')+"px";
                    var currentWidth = $(element).attr('data-currentwidth');
                    var currentHeight = $(element).attr('data-currentheight');
                    if(currentWidth === undefined || currentWidth === false || currentHeight === undefined || currentHeight === false) {
                        return true;
                    }
                    return (parseInt(currentWidth) === parseInt($(element).attr('data-width'))) 
                        && (parseInt(currentHeight) === parseInt($(element).attr('data-height')));
                },"Tamaño necesario no cumplido");

                // Ancho de imágen.
                $('input[type="file"][imgwidth="true"]').each(function(){
                    $(this).on('change',function(){
                        input = this;
                        $(input).removeAttr('data-currentwidth');
                        fr = new FileReader();
                        $(fr).load(function(){
                            var img = document.createElement('img');
                            $(img).css('display','none');
                            $(img).attr('src',fr.result);
                            $(img).load(function() {
                                $(input).attr('data-currentwidth',this.width);
                                $(this).parent().remove(img);
                            });
                            $(this).parent().append(img);
                        });
                        fr.readAsDataURL(input.files[0]);
                    });
                });

                jQuery.validator.addMethod('imgwidth', function(value, element, params){
                    jQuery.validator.messages.imgwidth = "Ancho: "+$(element).attr('data-width')+"px";
                    var currentWidth = $(element).attr('data-currentwidth');
                    if(currentWidth === undefined || currentWidth === false) {
                        return true;
                    }
                    return (parseInt(currentWidth) === parseInt($(element).attr('data-width')));
                },'Ancho necesario no cumplido');

                // Alto de imágen.
                $('input[type="file"][imgheight="true"]').each(function(){
                    $(this).on('change',function(){
                        input = this;
                        $(input).removeAttr('data-currentheight');
                        fr = new FileReader();
                        $(fr).load(function(){
                            var img = document.createElement('img');
                            $(img).css('display','none');
                            $(img).attr('src',fr.result);
                            $(img).load(function() {
                                $(input).attr('data-currentheight',this.height);
                                $(this).parent().remove(img);
                            });
                            $(this).parent().append(img);
                        });
                        fr.readAsDataURL(input.files[0]);
                    });
                });

                jQuery.validator.addMethod('imgheight', function(value, element, params){
                    jQuery.validator.messages.imgheight = "Alto: "+$(element).attr('data-height')+"px";
                    var currentHeight = $(element).attr('data-currentheight');
                    if(currentHeight === undefined || currentHeight === false) {
                        return true;
                    }
                    return (parseInt(currentHeight) === parseInt($(element).attr('data-height')));
                },'Alto necesario no cumplido');

                jQuery.validator.addMethod('phone', function(value, element, params){
                    var phone = /^9[0-9]{8}$/;
                    return phone.test(value);
                },'Telefono no válido');

                jQuery.validator.addMethod('mobile', function(value, element, params){
                    var mobile = /^6[0-9]{8}$/;
                    return mobile.test(value);
                },'Móvil no válido');

                jQuery.validator.addMethod('cp', function(value, element, params){
                    // return (!isNaN(value) && value >= 1001 && value <= 53000);
                    var es_pattern = /^[0-9]{5}$/;
                    var pt_pattern = /^[0-9]{4}-{0,1}[0-9]{3}$/;
                    var result = false;
                    result = es_pattern.test(value) || pt_pattern.test(value);
                    return result;
                },'Código Postal no válido');
                
                
                //START VALIDATE
                $("form[data-validate='true']").each(function(){ 
                    $(this).validate();
                });
                
            }
                
        }

        window.checkCaptcha = checkCaptcha;

        function checkCaptcha(){
            if(grecaptcha.getResponse().length === 0){
               $('input[data-validate=recaptcha]').val('');
            }else{
               $('input[data-validate=recaptcha]').val('ok');
               $('input[data-validate=recaptcha]').removeClass('error_bg');
               $('input[data-validate=recaptcha]').parent().removeClass('error_bg');
               $('input[data-validate=recaptcha] ~ .error').remove();
            }
        }
                
        // DeferLoad de Imágenes
        function deferload(){
            
            $('img.thumb:visible').each(function() {
                                
                if ($(this).hasClass('imgproducto')) {
                    var efotos = 'https://cdn2.depau.es/articulos/448/448/fixed/';
                    var nofotos = '/assets/img/articulos/448x448/no_foto.png';
                } else if ($(this).hasClass('imgcart')) {
                    var efotos = 'https://cdn2.depau.es/articulos/118/118/fixed/';
                    var nofotos = '/assets/img/articulos/30x30/no_foto.png';
                } else if ($(this).hasClass('imgmarca')) {
                    var efotos = 'https://cdn2.depau.es/marcas/';
                    var nofotos = '/assets/img/articulos/30x30/no_foto.png';
                } else if ($(this).hasClass('imglist') || $(this).closest('#products-list, .product-list').hasClass('display-list')) {
                    var efotos = 'https://cdn2.depau.es/articulos/60/60/fixed/';
                    var nofotos = '/assets/img/articulos/60x60/no_foto.png';
                } else if ($(this).closest('#products-list, .product-list').hasClass('display-grid')){
                    var efotos = 'https://cdn2.depau.es/articulos/160/160/fixed/';
                    var nofotos = '/assets/img/articulos/160x160/no_foto.png';
                } else if ($(this).closest('#pedidos-pendientes, .product-list').hasClass('display-list')){
                    var efotos = 'https://cdn2.depau.es/articulos/60/60/fixed/';
                    var nofotos = '/assets/img/articulos/60x60/no_foto.png';
                } 
                
                // Comprobamos si están cargadas
                if(!$(this).prop('loaded')){
                    $(this).attr('src', ''+efotos + $(this).parent().attr('data-defer-load')).on('error', function(){
                        $(this).attr('src', nofotos);
                    });			
                    $(this).prop('loaded', true);
                }
                	
            });
        }
        
        //Defer load de imágenes
        $('img[data-defer-load]').each(function(){
            $(this).attr('src', $(this).attr('data-defer-load')).on('error', function(){            
                if($(this).data('imagesize')){
                    $(this).attr('src', '/assets/img/no_foto/448/no_foto.png');
                }else if ($(this).hasClass('menu-commercial-img')){
                    $(this).attr('src', '/assets/img/fotos/'+'no_foto-llama.png');
                }else{
                    $(this).attr('src', '/assets/img/no_foto/'+$(this).attr('width')+'/no_foto.png');
                }
            });
        });
      
    
    ///////////////////////////////////////////////////////////////////////////////////
 /* AJAX EN LAS BUSQUEDAS * ////////////////////////////////////////////////////////
 * 
  */

    $(function() { // Forma corta para el document ready (Cuando toda la web y sus scripts esten cargados)
        

        var DSC = false;
        var procesar = false;
                             
        // Primero leemos el número de páginas que tiene la búsqueda
        var numero_de_paginas = 1; // Lo convertimos a número
        var contador_paginas = 2; // Es la página por la que comenzará a servir la paginación ajax
        var url_web = document.URL; // Leemos la ruta para saber en qué sección está y qué esta buscando
        var site_url = 'https://www.depau.es/';
        url_web = url_web.substring(site_url.length);
        // Si contiene parámetros le añadimos uno mas con el &, si no lo contiene lo empezamos con un ?. el pag se AÑADE como último parámetro.
        if( url_web.indexOf("?") >= 0) { var last_word = "&pag=";} else { var last_word =  "?pag=";} 
        var url_web = "ajaxrequest/listado/"+url_web; // Leemos la ultima parte del split de la URL para saber si contiene parámetros o si no
        
        
        
        function cargarArticulos(minimo, maximo){
            var maximo_real = $('#filtro_precio_maximo').data('real');
            var minimo_real = $('#filtro_precio_minimo').data('real');
            var minimoURL = (minimo.length > 0) ? "&minimo="+minimo : "&minimo="+minimo_real ;
            var maximoURL = (maximo.length > 0) ? "&maximo="+maximo : "&maximo="+maximo_real ;
            var soloStock = ($('input[name=soloStock]').val() !== '') ? "&soloStock=1" : '' ;
            var soloReaco = ($('input[name=soloReacondicionados]').val() !== '') ? "&soloReacondicionados=1" : '' ;
            
            var _numero_de_paginas = $("#listado-articulos").attr("data-paginas");
            var numero_de_paginas = parseInt(_numero_de_paginas);

            if(contador_paginas <= numero_de_paginas) {
                var contenedor = $('body').find(".nueva_carga_ajax");
                if(contenedor.length == 0){
                    $( document ).ajaxComplete(function( event, request, settings ) {
                        contenedor = $('body').find(".nueva_carga_ajax");
                    });
                }
                var urlFinal = site_url + url_web + last_word + contador_paginas + minimoURL + maximoURL + soloStock + soloReaco;
                urlFinal = urlFinal.replace(/ /g, '%20');
                contenedor.load( urlFinal +" #listado-articulos .product-list", function() {
                    $('.vista-favoritos + #listado-articulos .caja-fav[data-modal]').removeAttr('data-modal');
                    if(contador_paginas < numero_de_paginas) {
                        contenedor.removeAttr("class").append("<div class='nueva_carga_ajax'></div>"); /* Borramos la clase nueva_carga_ajax del div actual */
                    } else {
                        $("#spinner").hide();
                    }
                    contador_paginas++;
                    procesar = true;
                    localStorage.setItem("page",$('#listado-articulos').html());
                    var object = {
                        section: $('.breadcrumbs [itemprop=itemListElement]:last-child [itemprop=name]').html(), 
                        value: $('#listado-articulos').html(), 
                        timestamp: new Date().getTime(),
                        pages: contador_paginas,
                        dsc: DSC,
                        url: document.URL
                    };
                    localStorage.setItem("page", JSON.stringify(object));
                });
            } else {
                $("#spinner").hide();
            }
        }
        
        // Cargamos con AJAX la siguiente página hacia abajo
        $("#boton-cargar-mas").on("click", function(e) {
            var minimo = $('#formNuevaBusqueda input[name=minimo]').val();
            var maximo = $('#formNuevaBusqueda input[name=maximo]').val();
            cargarArticulos(minimo, maximo);
            $("#spinner").show();
            $("#boton-cargar-mas").remove();
        });
        $(window).scroll(function() {
            var minimo = $('#formNuevaBusqueda input[name=minimo]').val();
            var maximo = $('#formNuevaBusqueda input[name=maximo]').val();
            if(procesar){
                var alturaScroll = $(window).height();
                if(alturaScroll - $(window).scrollTop() < 200) {
                    procesar = false;
                    $("#spinner").show();
                    cargarArticulos(minimo, maximo);
                }
            }
        });
           
        /// VER COMERCIAL
        $("#menu-commercial-close").on("click", function(e){
        $("#comercial_asignado").toggleClass("comercial-visible");

          if ($("#menu-commercial-close").children().children('i').hasClass('fa-angle-left')) {
                     $('#menu-commercial').addClass("comercial-visible");
                     $("#menu-commercial-close").children().html("Mi Asistente Comercial <i class='fa-depau fa-angle-right '></i>");
                 } else {
                     $('#menu-commercial').removeClass("comercial-visible");
                     $("#menu-commercial-close").children().html("<i class='fa-depau fa-angle-left'></i>");
                 }
                 $.ajax({
                     url: '/ajaxrequest/toggle_menu_commercial',
                     async: true,
                     dataType: 'json',
                 });


        e.stopPropagation();
        });

        $("input[name=cantidad]").on("focus", function(e){
            var nov = $(this).parent().parent().attr('class');
            $($(this).parent().parent()).addClass("magic");
        }); 

        $("input[name=cantidad]").on("blur", function(e){
            var nov = $(this).parent().parent().attr('class');
            $($(this).parent().parent()).removeClass("magic");
        }); 
        $("#listado-articulos, .listado-articulos").on("click", '.product-description > div[data-url], div.product-description[data-url]', function(e){
            //prevenirClick = false;
            var elementoOriginal = e.originalEvent.target;
            if (typeof elementoOriginal.attributes['data-prevenir-click'] !== 'undefined' && elementoOriginal.attributes['data-prevenir-click'].nodeValue == "true"){
                mostrar_promociones(elementoOriginal);
                //prevenirClick = true;
            }
            //if(elementoOriginal.localName != 'button' && !prevenirClick){
                //var ruta = $(this).attr('data-url');
                //window.location.href = ruta;
            //}
        });  
        
        function mostrar_promociones(elemento){
            
            var articulo = elemento.attributes['data-articulo'].nodeValue;
            
            $.ajax({
                url: '/ajaxrequest/ajax_mostrar_promociones',
                type: 'POST',
                async: false,
                dataType: 'json',
                data: { articulo: articulo },
                success: function(data, textStatus, jqXHR) {
                    if(data != false){
                     
                        var promociones = data.codigos_promocionales;
                        
                        var strModal = "\
                            <div id='fade' class='overlay'></div>\n\
                            <div id='light'>\n\
                                <span id='light-close'><i class='fal fa-times'></i></span>\n\
                                <div id='modal-light-container' class='border-radius-all'>\n\
                                    <div id='light-wrap' class='light-wrap'>\n\
                                        <div class='light-header border-radius-top'>\n\
                                        </div>\n\
                                        <div id='contenido_modal'>\n\
                                            <h2 class='cabecera-modal-nueva'>Depau Sistemas S.L.</h2>\n\
                                            <p class='margin-top-xl text-center'> Este artículo tiene las siguientes promociones disponibles:</p>";
                                            $.each(promociones, function(key, observaciones){
                                                observaciones = observaciones.replace('\u00a4', '&euro;');
                                                strModal += "<p class='text-center'>"+observaciones+"</p>";
                                            });
                                            strModal += "<p class='botones-modal'>\n\
                                                <a class='btn tercio btn-d-primary close-modal'>Cerrar</a>\n\
                                            </p>\n\
                                        </div>\n\
                                    </div>\n\
                                </div>\n\
                            </div>"; 
                        removeModal();
                        $('body').prepend(strModal);
                        showModal();
                        closeModal();                     
                     
                    }
                }
            });            
            
        }
        
        $('#activarMascara').click(function(){
            localStorage.removeItem("page");
        });
        if (typeof(Storage) !== "undefined") {
            if(localStorage.getItem("page") !== null) {
                var page = JSON.parse(localStorage.getItem("page")),
                    dateString = page.timestamp,
                    now = new Date().getTime().toString();

                if(page.dsc != DSC){
                    localStorage.removeItem("page");
                }else if(now - dateString > 1200000){  //Caducidad de tiempo 20 minutos
                    localStorage.removeItem("page");
                }
            }
            
            var navegacionAnterior = JSON.parse(localStorage.getItem("navegacion"));
            if (navegacionAnterior === null) {
                navegacionAnterior = {
                    paginaActual: '', 
                    paginaAnterior: '', 
                    paginaAnteriorAnterior: ''
                };
            }
            var navegacion = {
                paginaActual: document.URL, 
                paginaAnterior: navegacionAnterior.paginaActual, 
                paginaAnteriorAnterior: navegacionAnterior.paginaAnterior
            };
            localStorage.setItem("navegacion", JSON.stringify(navegacion));

            if(navegacion.paginaAnteriorAnterior == navegacion.paginaActual && navegacion.paginaAnterior != navegacion.paginaActual ){
                if(localStorage.getItem("page") != null && page.url == document.URL && $('.breadcrumbs [itemprop=itemListElement]:last-child [itemprop=name]').html() == page.section){
                    $('#listado-articulos').html(page.value);
                    contador_paginas = page.pages;
                    procesar = true;
                }   
                
                var url = navegacion.paginaAnterior;
                url = url.split('/');
                var urlArticulo = url[url.length-1];
                if(urlArticulo.length > 0){
                    var articulo = $('#listado-articulos').find('.item[data-url="'+urlArticulo+'"]');
                    if(articulo.length > 0){
                        var altura = articulo.offset().top;
                        var altura_centrada = altura - ($(window).height() / 2);
                        $('html, body').animate({scrollTop: altura_centrada}, 0);
                        articulo.addClass("hover");
                    }
                }
            }else if(navegacion.paginaAnterior == navegacion.paginaActual){
                localStorage.removeItem("page");
            }

            $(window).mousemove(function() {
                $('#listado-articulos .item').removeClass('hover');
            });
        }
    });       
        
    //AddToCart (Sin Modal)
    $('[data-addtocart]').click(function(e){
        e.preventDefault();

        var codigo = $(this).data('codigo');
        var cantidad = parseInt($("input[name='cantidad'][data-codigo='" + codigo + "']").val());
        var deposito = $(this).data('deposito');

        $.post('/ajaxrequest/addtocart', { articulo: codigo, cantidad: cantidad, deposito:deposito }, function(data){
            $("#carrito_drop .lenguetas-number").html(data.articulos); 
        }, 'json');
    });   

