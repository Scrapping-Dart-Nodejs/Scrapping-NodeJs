        $("input[name='cantidad'][data-codigo='" + $(this).attr('data-codigo') + "']").focus();
        $("input[name='cantidad'][data-codigo='" + $(this).attr('data-codigo') + "']").select();
    });
    

                
        //Navigation Grid/List
        $('body').on('click', '.switch-list, .switch-grid', function(){
            var options = { block: $('#listado-articulos').data('block'), view: ($(this).hasClass('switch-list')) ? 'list' : 'grid'};
            $.post('/ajaxrequest/storenavigation', options, function(data){
                if(data){
                    if( options.view === 'list'){
                        $(".switch-list").addClass('active');
                        $(".switch-grid").removeClass('active');
                        $(".grid-list .product-list").removeClass('display-grid').addClass('display-list');

                    }else{
                        $(".switch-grid").addClass('active');
                        $(".switch-list").removeClass('active');
                        $(".grid-list .product-list").removeClass('display-list').addClass('display-grid');	
                    }
                    $('img.thumb').each(function() { $(this).prop('loaded', false); });
                    deferload();
                }
            },'json');
        });

        deferload();
    
    $('.colwrap').on('click', '[data-id-fav]', function(){
        
        var controlVistaFavoritos = $('.vista-favoritos');
        if(controlVistaFavoritos.length == 0){

            $(this).toggleClass('fal');
            $(this).toggleClass('fas');

            var action = 'add';
            if($(this).hasClass('fal')){
                action = 'remove';
            }

            var csrf_token = $("input[name='csrf_token']").val();

            var tooltip = $(this).siblings('.top-tooltip');

            var options = { codigo: $(this).data('id-fav'), action: action, csrf_token: csrf_token };
            $.post('/ajaxrequest/toggleFav', options, function(data){
                if(data){
                    
                var strModal = "\
                    <div id='fade' class='overlay'></div>\n\
                    <div id='light'>\n\
                        <span id='light-close'><i class='fal fa-times'></i></span>\n\
                        <div id='modal-light-container' class='border-radius-all'>\n\
                            <div id='light-wrap' class='light-wrap'>\n\
                                <div class='light-header border-radius-top'>\n\
                                </div>\n\
                                <div id='contenido_modal'>\n\
                                    <h2 class='cabecera-modal-nueva'>Depau Sistemas S.L.</h2>";
                    
                    /* Actualizamos el tooltip */
                    if(action == 'add'){
                        tooltip.attr('data-texto', 'Eliminar de Favoritos');
                        strModal += "\<p class='margin-top-xl text-center'>Producto añadido de tu lista de Favoritos</p>";
                    }else{
                        tooltip.attr('data-texto', 'Añadir a Favoritos');
                        strModal += "\<p class='margin-top-xl text-center'>Producto eliminado de tu lista de Favoritos</p>";
                    }

                    strModal += "\<p class='botones-modal'>\n\
                                    <a class='btn tercio btn-d-primary close-modal'>Aceptar</a>\n\
                                </p>\n\
                            </div>\n\
                        </div>\n\
                    </div>\n\
                </div>";
                removeModal();
                $('body').prepend(strModal);
                console.log('a');
                showModal();
                console.log('b');
                closeModal();     
                    
                }
            }, 'json');

            
        }
    });
    
    $(window).on('resize touchmove scroll', function () {
        reajustarMenuResponsive();
    });
    $('#mostrar-menu-lateral.responsive-sm').click(function(){
        reajustarMenuResponsive();
    });
    function reajustarMenuResponsive(){
        $('#accordion').addClass('hover-activo');
        var altura = window.innerHeight;
        var navBarFija = $('body').hasClass('navBarFija');
        if(!navBarFija){
            var posicion = $('.colmask').offset().top;
        }else{
            var posicion = $('#menuResponsive').height();
            $(".user-menu").removeClass('active');
            $('.label').removeClass('lenghover');
            $('.label').css("color", "#a1a1a1");
            $('.letra_circulo').removeClass('backhover');
            $('.letra_circulo').css("background", "#6e83b4");
        }
        var alturaDisponible = altura - posicion;
        $('#accordion').css('max-height', alturaDisponible).css('min-height', alturaDisponible);
    }
   
    $('#menu-lateral.responsive-oculto .grupo').hover(function(){
        $('#accordion').addClass('hover-activo');
        var alturaInicial = $(this).children('.cat-panel').attr('data-altura');
        var altura = $(window).height();
            var posicion = $('.colmask').offset().top;
        var alturaDisponible = altura - posicion;
        $('#accordion').css('max-height', alturaDisponible).css('min-height', alturaDisponible);
        if(alturaInicial > alturaDisponible){
            $(this).children('.cat-panel').css('max-height', alturaDisponible).css('min-height', alturaDisponible);
        }else{
            $(this).children('.cat-panel').css('max-height', alturaInicial).css('min-height', alturaInicial);
        }
    });
    $('#menu-lateral').mouseleave(function(){
        $('.categorias.menu-fixed').removeClass('hover-activo');
    });

    $('body').on('click', '.descuento_axb', function(){
        var input = $(this).children('input');
        var unidades = $(input).data('axb');
        var codigo = $(input).data('codigo');
        if($(input).prop('checked')){
            $('input[name="cantidad"][data-codigo="'+codigo+'"]').val(unidades);
        }else{
            $('input[name="cantidad"][data-codigo="'+codigo+'"]').val(1);
        }
        //Descomentar para añadir automaticamente al carrito
        //$('input[name="cantidad"][data-codigo="'+codigo+'"] ~ span [type="submit"][data-codigo="'+codigo+'"]').trigger('click');
    });

    
$('#imprimir').click(function(){
            $('#mascara-nombre-cliente').html('<h2>'+$('#nombre-cliente').val()+'</h2>');
            hideModalForm('form_nombre_cliente');
            print();
	});    
        
        function showModal(){
            $('#fade, #light').show();
            $('#fade').addClass('visible');
        }
        function removeModal(){
            $('#fade, #light').remove();
        }        
        function closeModal(){
            $('#fade, #light-close, .close-modal').click(function(){
                removeModal();
            });
        }     
        function guardarCookies(){
            $('.guardar_cookies').click(function(){
                var data = {
                    necesarias: true, 
                    rendimiento: true,
                    funcionalidad: false,
                    focalizacion: false,
                };
                if($('input[name=funcionalidad]').prop("checked") === undefined || $('input[name=funcionalidad]').prop("checked")){
                    data.funcionalidad = true;
                }
                if($('input[name=focalizacion]').prop("checked") === undefined || $('input[name=focalizacion]').prop("checked")){
                    data.focalizacion = true;
                }
                
                $.ajax({
                    url: '/ajaxrequest/accept_cookies',
                    type: 'POST',
                    async: true,
                    timeout: 5000,
                    dataType: 'json',
                    data: data,
                    success: function(data, textStatus, jqXHR) {
                        removeModal();
                        $('#cookies-policy-container').remove();
                        location.reload();
                    },
                });                
                
                
            });
        }
        
        $('#boton-aceptar-cookies').click(function(){
                var data = {
                    necesarias: true, 
                    rendimiento: true,
                    funcionalidad: false,
                    focalizacion: false,
                };
                
                if($('input[name=funcionalidad]').prop("checked") === undefined || $('input[name=funcionalidad]').prop("checked")){
                    data.funcionalidad = true;
                }
                if($('input[name=focalizacion]').prop("checked") === undefined || $('input[name=focalizacion]').prop("checked")){
                    data.focalizacion = true;
                }
                $.ajax({
                    url: '/ajaxrequest/accept_cookies',
                    type: 'POST',
                    async: true,
                    timeout: 5000,
                    dataType: 'json',
                    data: data,
                    success: function(data, textStatus, jqXHR) {
                        $('#cookies-policy-container').remove();
                        location.reload();
                    },
                });                
                
        });
        
            // Formulario de acceso a máscara
            $('#configuracion_cookies').click(function(e){
                e.preventDefault();        
                var strModal = "\
                    <div id='fade' class='overlay'></div>\n\
                    <div id='light'>\n\
                        <span id='light-close'><i class='fal fa-times'></i></span>\n\
                        <div id='modal-light-container' class='border-radius-all'>\n\
                            <div id='light-wrap' class='light-wrap'>\n\
                                <div class='light-header border-radius-top'>\n\
                                </div>\n\
                                <div id='contenido_modal'>\n\
                                    <h2 class='cabecera-modal-nueva'>Configuración de Cookies</h2>\n\
                                    <p class='margin-top text-left'> Selecciona la opción de cookies que prefieras para navegar en depau.es: </p>\n\
                                    <h2> Funcional (Obligatorias): </h2>\n\
                                    <p style='text-align: justify;'>\n\
                                        <input type='checkbox' name='necesarias' value='s' class='inline' checked='checked' disabled='disabled'>\n\
                                        <strong>Para usos estrictamente necesarios:</strong> Estas cookies son esenciales a fin de que usted navegue por el sitio web y utilizar sus funciones, tales como el acceso a áreas seguras del sitio Web, recordar los artículos de tu lista de la compra, recordar en qué punto del pedido te encuentras.\n\
                                    </p>\n\
                                    <p style='text-align: justify;'>\n\
                                        <input type='checkbox' name='rendimiento' value='s' class='inline' checked='checked' disabled='disabled'>\n\
                                        <strong>Rendimiento:</strong> Las cookies recopilan información acerca de cómo los visitantes usan un sitio web, por ejemplo, páginas que los visitantes van más a menudo y si reciben mensajes de error. Estas cookies se utilizan para mejorar un sitio web en versiones futuras, mejorar la experiencia del usuario, mejorar el funcionamiento del sitio web reduciendo el tiempo que tardan en cargarse las páginas que visitas.\n\
                                    </p>\n\
                                    <h2> Personalización (Opcionales): </h2>\n\
                                    <p style='text-align: justify;'>\n\
                                        <input type='checkbox' name='funcionalidad' value='s' class='inline' checked='checked'>\n\
                                        <strong>Funcionalidad:</strong> Estas cookies permiten que el sitio web recuerde decisiones que usted toma accediendo a él, como su nombre de usuario o el idioma y proporcionar una mejor experiencia de usuario. garantizar que el aspecto de la página web es coherente, garantizar tu seguridad cuando inicias sesión.\n\
                                    </p>\n\
                                    <p style='text-align: justify;'>\n\
                                        <input type='checkbox' name='focalizacion' value='s' class='inline' checked='checked'>\n\
                                        <strong>La focalización o la publicidad:</strong> Estas cookies se utilizan para entregar el contenido más relevante para usted y sus intereses.\n\
                                    </p>\n\
                                    <p class='margin-top-xl text-center'>\n\
                                        <a class='btn tercio btn-d-primary guardar_cookies'>Aceptar y guardar</a>\n\
                                    </p>\n\
                                </div>\n\
                            </div>\n\
                        </div>\n\
                    </div>";                     
                removeModal();
                $('#cookies-policy-container').prepend(strModal);
                showModal();
                closeModal();
                guardarCookies();
            });
                    
        
        
    

    
        //Para linkar la fila
        $('#accordion  div.grupo').click(function(e){ 
            if(!$(e.target).hasClass('texto') && !$(e.target).hasClass('cat-more')){
                location.href = $(this).find('a').attr('href'); 
            }
        });    
     
        //Ahora si es indispensable usar js para adaptar el menu
        var alturaInicial = 533;
        $('.cat-more').click(function(e){
            e.preventDefault();
            $('.mostrando-ocultos').removeClass('mostrando-ocultos');
            $('.ocultando-ocultos').removeClass('ocultando-ocultos');
            var elemento = $(this).parent();
            var grupo = $(elemento).parent();
            var alturaActual = $(grupo).height();
            var panel = $(grupo).parent();
            var alturaPanel = parseInt($(panel).css("height").replace('px', ''));;
            if($(elemento).hasClass('mostrar-ocultos')){
                /* Cambiamos el texto */
                $(this).children('.texto').html('Ver más');
                $(this).children('i').removeClass('fa-long-arrow-alt-left');
                $(this).children('i').addClass('fa-long-arrow-alt-right');
                
                /* Revisamos si tendremos que reducir el menu */
                var columna = $(grupo).data('columna');
                $(panel).addClass('target');
                var alturaRealColumna = 0;
                $('.target ul[data-columna='+columna+']').each(function(){
                    alturaRealColumna += $(this).height();
                });
                $(panel).removeClass('target');
                
                var achicarMenu = false;
                if(alturaPanel == (alturaRealColumna+55)){
                    achicarMenu = true;
                }
                
                /* Ocultamos los elementos */
                $(elemento).removeClass('mostrar-ocultos');
                
                var nuevaAltura = $(grupo).height();
                var diferencia = nuevaAltura-alturaActual;
                
                $(grupo).addClass('ocultando-ocultos');
                
                $('ul.ocultando-ocultos ~ ul[data-columna='+columna+']').each(function(){
                    var top = $(this).css("top");
                    var posicion = parseInt(top.replace('px', ''));
                    var nuevaAltura = posicion+diferencia;
                    $(this).css("top", nuevaAltura+'px');
                }); 
                
                if(achicarMenu){
                    var height = parseInt($(panel).css("height").replace('px', ''));
                    if(height+diferencia < alturaInicial){
                        $(panel).css("height", alturaInicial+'px');
                    }else{
                        $(panel).css("height", (height+diferencia)+'px');
                    }
                }
                
            }else{
                $(this).children('.texto').html('Ver menos');
                $(this).children('i').addClass('fa-long-arrow-alt-left');
                $(this).children('i').removeClass('fa-long-arrow-alt-right');
                
                $(elemento).addClass('mostrar-ocultos');
                var nuevaAltura = $(grupo).height();
                var diferencia = nuevaAltura-alturaActual;
                
                var columna = $(grupo).data('columna');
                $(grupo).addClass('mostrando-ocultos');
                
                $('ul.mostrando-ocultos ~ ul[data-columna='+columna+']').each(function(){
                    var top = $(this).css("top");
                    var posicion = parseInt(top.replace('px', ''));
                    var nuevaAltura = posicion+diferencia;
                    $(this).css("top", nuevaAltura+'px');
                }); 
                
                
                
                $(panel).addClass('target');
                var alturaRealColumna = 0;
                $('.target ul[data-columna='+columna+']').each(function(){
                    alturaRealColumna += $(this).height();
                });
                $(panel).removeClass('target');
                if(alturaPanel < alturaRealColumna+55){
                    $(panel).css("height", (alturaRealColumna+55)+'px');
                }
                
            }
        });
    
    

    

    //Imprimimos
    $('#print').click(function(){
        
        $('#container').removeClass('container');

        if($('div.holder').length> 0){  
            $(".product-list li:nth-child(15n+1)").each(function(i){
                if(i > 0){ $(this).addClass('page-break'); }
            });
        }
      
        if( $(".product-list ").length > 0){
            
            if( $(".product-list").hasClass('display-grid') ) {
                $(".product-list").removeClass('display-grid').addClass('display-list');
                print();
                $(".product-list").removeClass('display-list').addClass('display-grid');
            }else{
                print();
            }
        }else{
            print();
        }
        $('#container').addClass('container');

    });
    
    
    
                      
    

        sessionStorage.clear();

        $(window).scroll(function(){
            if($("#menu").hasClass("responsive-visible")){
                $("#menu").removeClass("responsive-visible");
                $("#menu").addClass("responsive-oculto");
            }
            if($("#responsive-menu-cabecera-usuario").hasClass("responsive-visible")){
                $("#responsive-menu-cabecera-usuario").removeClass("responsive-visible");
                $("#responsive-menu-cabecera-usuario").addClass("responsive-oculto");
            }

        });
        /* SCROLL TO TOP */
        $(window).scroll(function(){
            if($(window).scrollTop() > 200) {
                $("#scrollToTop" ).addClass("scroll-visible");
            }else{
                $("#scrollToTop").removeClass("scroll-visible");
            }  
        });
        //Scroll Top
        $("#scrollToTop").on('click', function(e) {
            e.preventDefault();
            $("#scrollToTop").removeClass("scroll-visible");
            $('html, body').animate({scrollTop: 0}, 400);
        });
        
        $('#header-btn-entrar').click(function(){
           $('.page').toggleClass('log-visible');
           var texto = $('#header-btn-entrar > span');
           texto.html(texto.html() == "Entrar" ? "Ocultar" : "Entrar")    
           
        });

    
                    
    
    function lanzarBuscador(){
        var palabra = $('#buscador-principal input').val();
        var url = '/';
        window.location.href = url+'buscar?q='+palabra;
    }
    $('#buscador-principal .btn').click(function(){
        var palabra = $('#buscador-principal input').val();
        var longitud = palabra.length;
        if(longitud >= 2){
            lanzarBuscador();
        }
    }); 
    
    $('#buscador-principal').on('keyup keypress', function(e) {
        var keyCode = e.keyCode || e.which;
        var palabra = $('#buscador-principal input').val();
        var longitud = palabra.length;
        if(keyCode === 13) { 
            e.preventDefault();
            if (longitud >= 2){
                lanzarBuscador();
            }
        }
    });
    
    $(function() {
        
        $(".mostrar_menu").on("click", function(e) {
        $(".page").toggleClass("menu-visible");
        $('.mostrar_menu .fas.pull-right').toggleClass('fa-chevron-circle-left');
        $('.mostrar_menu .fas.pull-right').toggleClass('fa-chevron-circle-right');
        $.ajax({
            url: '/ajaxrequest/toggle_menu_normal',
            async: true,
            dataType: 'json',
        });
        
        });
        
    }); 
    
    

    function stikyMenu(){
        var menu = $('.page #menu-lateral .categorias').height();
        var ancho = $(window).width();
        if(menu > 0 && ancho >= 812){
            var ventana = $(window).height();
            var cabecera = $('.header-container:not(.bg-white)').height();
            var footer = $('.footer-container').height();
            /*if(ventana >= menu + cabecera){*/
                $('.page.menu-visible #menu-lateral').css({ height: (ventana - cabecera - footer) });
                $('.categorias').css({position: 'fixed', width: '230px', top: cabecera, bottom: 0, background: '#fff'});
                $('.footer-container').css({padding: '0 0 0 230px'});
            /*}*/
        }else{
            $('.footer-container').css({padding: '0 0 0 0'});
        }
        
    }
    /*stikyMenu();*/

            
    $(".comercial_asignado").on("click", function(){ $("#comercial_asignado").toggleClass("comercial-visible");});

    
    
    $("#mostrar-menu-herramientas").click(function(){
        if($("#responsive-menu-cabecera-usuario").hasClass("responsive-visible")){
            $("#responsive-menu-cabecera-usuario").removeClass("responsive-visible");
            $("#responsive-menu-cabecera-usuario").addClass("responsive-oculto");
        }
        if($("#menu-lateral").hasClass("responsive-visible")){
            $("#menu-lateral").removeClass("responsive-visible");
            $("#menu-lateral").addClass("responsive-oculto");
            $("body").removeClass("responsive-menu-visible");
        }
        if($("#menu").hasClass("responsive-oculto")){
            $("#menu").removeClass("responsive-oculto");
            $("#menu").addClass("responsive-visible");
        }else{
            $("#menu").removeClass("responsive-visible");
            $("#menu").addClass("responsive-oculto");
        }
    });
    
    /* PERFIL */
    $("#mostrar-menu-perfil").click(function(){
        if($("#menu").hasClass("responsive-visible")){
            $("#menu").removeClass("responsive-visible");
            $("#menu").addClass("responsive-oculto");
        }
        if($("#menu-lateral").hasClass("responsive-visible")){
            $("#menu-lateral").removeClass("responsive-visible");
            $("#menu-lateral").addClass("responsive-oculto");
            $("body").removeClass("responsive-menu-visible");
        }
        if($("#responsive-menu-cabecera-usuario").hasClass("responsive-oculto")){
            $("#responsive-menu-cabecera-usuario").removeClass("responsive-oculto");
            $("#responsive-menu-cabecera-usuario").addClass("responsive-visible");
        }else{
            $("#responsive-menu-cabecera-usuario").removeClass("responsive-visible");
            $("#responsive-menu-cabecera-usuario").addClass("responsive-oculto");
        }
    });
    $(window).scroll(function(){
        if($("#menu").hasClass("responsive-visible")){
            $("#menu").removeClass("responsive-visible");
            $("#menu").addClass("responsive-oculto");
        }
        if($("#responsive-menu-cabecera-usuario").hasClass("responsive-visible")){
            $("#responsive-menu-cabecera-usuario").removeClass("responsive-visible");
            $("#responsive-menu-cabecera-usuario").addClass("responsive-oculto");
        }
        
    });
    /* SCROLL TO TOP */
        $(window).scroll(function(){
            if($(window).scrollTop() > 200) {
                $("#scrollToTop" ).addClass("scroll-visible");
            }else{
                $("#scrollToTop").removeClass("scroll-visible");
            }  
        });
        /*Scroll Top*/
        $("#scrollToTop").on('click', function(e) {
            e.preventDefault();
            $("#scrollToTop").removeClass("scroll-visible");
            $('html, body').animate({scrollTop: 0}, 400);
        });

    /* FAMILIAS */
    $("#mostrar-menu-lateral, .cover").click(function(){
        var altura = 13;
        if($('.breadcrumbs').height() > 0){
            altura = 20 - $('.breadcrumbs').height();
        }
        $('#menu-lateral').attr('style', 'margin-top: '+altura+'px;');
        if($("#menu").hasClass("responsive-visible")){
            $("#menu").removeClass("responsive-visible");
            $("#menu").addClass("responsive-oculto");
        }
        if($("#responsive-menu-cabecera-usuario").hasClass("responsive-visible")){
            $("#responsive-menu-cabecera-usuario").removeClass("responsive-visible");
            $("#responsive-menu-cabecera-usuario").addClass("responsive-oculto");
        }
        if($("#menu-lateral").hasClass("responsive-oculto")){
            $("#menu-lateral").removeClass("responsive-oculto");
            $("#menu-lateral").addClass("responsive-visible");
            $("body").addClass("responsive-menu-visible menu-visible");
            $(".page").addClass("menu-visible");
        }else{
            $("#menu-lateral").removeClass("responsive-visible");
            $("#menu-lateral").addClass("responsive-oculto");
            $("body").removeClass("menu-visible responsive-menu-visible");
        }
    });
    
    $("#imagen-grupo-familias-imagen").ready(function() {
        if($("#imagen-grupo-familias-imagen").height() < 50 ){
            $("#imagen-grupo-familias").html("");
        }
        else{
            $("#imagen-grupo-familias").css('height', 'auto');
            $("#imagen-grupo-familias-imagen").css('visibility', 'visible');
        }
    });
    
    $(window).on("scroll", function() {
        var width = window.innerWidth;
        var limit = 812;
        var altura = $('.header-container:not(.bg-white)').height();
        var alturaCabecera = 13;
        if($('.breadcrumbs').height() > 0){
            alturaCabecera = 20 - $('.breadcrumbs').height();
        }
        if  (width <= limit){
            //console.log('w: '+window.pageYOffset);
            //console.log('a: '+altura);
            if (window.pageYOffset >= altura) {
                $('body').addClass("navBarFija");
                var alturaMenu = window.innerHeight - $('#menuResponsive').height();
                $('#menu-lateral').attr('style', 'max-height:'+alturaMenu+'px');
            } else {
                $('#menu-lateral').attr('style', 'margin-top: '+alturaCabecera+'px;');
                $('body').removeClass("navBarFija");
            }
        }
    });



    

        /*Bajo pedido*/
        $('.bajopedido div, .liquidacion div').click(function(e){
            $("input[name='cantidad']").focus();
            $("input[name='cantidad']").select();
        });

        $('.pestana').click(function(){
            $('.pestana').removeClass('active');
            $(this).addClass('active');
        });


        $(document).ready(function(){
            $('.foto-miniatura').css('opacity', 1);
            $('.foto-miniatura').css('display', 'block');

            $('.foto-miniatura').owlCarousel({
                loop:false,
                items: 5,
                margin: 0,
                nav:false,
                mouseDrag: false
            })

        });

        $('body').on('click', '[data-valoration]', function(){
            var valor = $(this).data('valoration');
            var opinion = $(this).data('id');

            $.ajax({
                url: '/ajaxrequest/voteOpinionArticulo',
                type: 'POST',
                async: false,
                dataType: 'json',
                data: { id: opinion, valor: valor },
                success: function(data, textStatus, jqXHR) {
                    if(data){
                        alert(data.mensaje);
                    }
                }
            });
        });

        $('a[href="#opiniones"], #mejor_valoracion').click(function(e){
            $('span[href="#opiniones"]').children().click();
            var altura = $('div[class="pestanas"]').offset().top;
            $('html, body').animate({
              scrollTop: altura - 140
            }, 400);
        });

    
    
        $(".miniatura .imgproducto").on("click", function() {
            var id = $(this).data('img-id');
            var imagen_src = getImagenUrl(id);

            $("#img_modal_zoom").removeData();
            $("#imgproducto").removeData();

            $("#img_modal_zoom").attr('data-imagen-id', id);
            $("#imgproducto").attr('data-imgproducto', id);

            $("#img_modal_zoom").attr('src', imagen_src);
            $("#imgproducto").attr('src', imagen_src);
        });

        $("#imgproducto").on("click", function() {
            var id_imagen = $(this).data("imgproducto");
            var imagen_src = getImagenUrl(id_imagen);

            $("#img_modal_zoom").attr("src", imagen_src);
            $("#imagen_zoom").fadeIn(50);
        });

        $(".img_articulo + div img").on("click", function() {
            var id_imagen = $("#img_modal_zoom").data("img-id");
            var imagen_src = getImagenUrl(id_imagen);

            $("#img_modal_zoom").attr("src", imagen_src);
            $("#imagen_zoom").fadeIn(50);
        });

        $("#imagen_zoom").on("click", function() {
            $("#imagen_zoom").fadeOut(50);
        });

        //Funcion para obtener la url correcta, la otra alternativa es crear en JS la funcion de getRutaFotos
        function getImagenUrl(id_imagen){
            switch(id_imagen) {
                case 5:
                    return 'https://cdn2.depau.es/articulos/800/800/fixed/art_asu-pb%20prime%20h510m-e_5.jpg';
                    break;
                case 4:
                    return 'https://cdn2.depau.es/articulos/800/800/fixed/art_asu-pb%20prime%20h510m-e_4.jpg';
                    break;
                case 3:
                    return 'https://cdn2.depau.es/articulos/800/800/fixed/art_asu-pb%20prime%20h510m-e_3.jpg';
                    break;
                case 2:
                    return 'https://cdn2.depau.es/articulos/800/800/fixed/art_asu-pb%20prime%20h510m-e_2.jpg';
                    break;
                default:
                    return 'https://cdn2.depau.es/articulos/800/800/fixed/art_asu-pb%20prime%20h510m-e_1.jpg';
            }
        }

        $( window ).resize(function(e) {
            if ($(window).width() <= $(window).height()) {
                $("#img_modal_zoom").removeClass("zoom_apaisado");
                $("#img_modal_zoom").addClass("zoom_vertical");
            } else {
                $("#img_modal_zoom").removeClass("zoom_vertical");
                $("#img_modal_zoom").addClass("zoom_apaisado");
            }
       });

       $( window ).resize();

    
        });
    </script>
